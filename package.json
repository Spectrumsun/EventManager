{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production sequelize db:migrate && npm run build && npm run webpack:build && node build/server",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit --compilers  js:babel-core/register server/test",
    "migrations": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --fix server",
    "dev": "cross-env NODE_ENV=development npm run dev:start",
    "dev:start": "concurrent --kill-others \"npm run webpack:watch\" \"nodemon --ignore lib --exec  babel-node server/server",
    "clean": "rm -rf build && mkdir build",
    "build:server": "babel ./server -d ./build ./server -s",
    "build": "npm run clean && npm run build:server",
    "webpack:watch": "webpack --watch",
    "webpack:build": "webpack --config webpack.prod.config.js --progress --profile --color",
    "client:test": "jest"
  },
  "author": "Taiwo Sunday",
  "license": "ISC",
