{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production sequelize db:migrate && npm run build && npm run webpack:build && node build/server",
    "test": "NODE_ENV=test npm run migrations && NODE_ENV=test npm run start:test",
    "nyc:run": "nyc --reportrer=html --reporter=text",
    "start:test": "nyc --reportrer=html --reporter=text mocha --timeout 50000 --exit --require babel-core/register server/test/**/*.test.js",
    "migrations": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --fix server",
    "dev": "cross-env NODE_ENV=development npm run dev:start",
    "dev:start": "concurrently --kill-others \"npm run webpack:watch\" \"nodemon --ignore lib --exec  babel-node server/server\"",
    "clean": "rm -rf build && mkdir build",
    "build:server": "babel ./server -d ./build ./server -s",
    "dev:server": "nodemon --ignore lib --exec  babel-node server/server",
    "build": "npm run clean && npm run build:server",
    "webpack:watch": "webpack --watch --config webpack.dev.config.js",
    "webpack:build": "webpack --config webpack.prod.config.js --progress --profile --color",
    "test:client": "jest && codecov",
    "coverages": "npm run coverage && npm run test:client"
  },
  "author": "Taiwo Sunday",
  "license": "ISC",
